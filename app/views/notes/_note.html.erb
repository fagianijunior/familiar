<div class="col-lg-4">
    <div class="pull-left thumb text-info text-center"><h1>Aa</h1></div>
    <div class="pull-left">
        <h4><a href="<%= note_path(note) %>"><%= note.title %></a></h4>
        <% if note.date.present? %>
        <h5 class="text-muted">Note: <%= note.date %></h5>
        <% else %>
        <h5 class="text-muted">Note</h5>
        <% end %>
    </div>
    <div class="clearfix"></div>
</div>